<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">



	<bean id="clientsService" class="com.belhard.webappbank.service.impl.ClientsServiceImpl">
		<property name="clientsDao" ref="clientsDao" />
	</bean>

	<bean id="clientInfService"
		class="com.belhard.webappbank.service.impl.ClientInfServiceImpl">
		<property name="clientInfDao" ref="clientInfDao" />
	</bean>

	<bean id="accountsService" class="com.belhard.webappbank.service.impl.AccountsServiceImpl">
		<property name="accountsDao" ref="accountsDao" />
		<property name="clientInfDao" ref="clientInfDao" />
	</bean>

	<bean id="cardsService" class="com.belhard.webappbank.service.impl.CardsServiceImpl">
		<property name="cardsDao" ref="cardsDao" />
		</bean>



</beans>

